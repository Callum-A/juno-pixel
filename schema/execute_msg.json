{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "draw"
      ],
      "properties": {
        "draw": {
          "type": "object",
          "required": [
            "chunk_x",
            "chunk_y",
            "color",
            "x",
            "y"
          ],
          "properties": {
            "chunk_x": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "chunk_y": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "color": {
              "$ref": "#/definitions/Color"
            },
            "x": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "y": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_admin"
      ],
      "properties": {
        "update_admin": {
          "type": "object",
          "required": [
            "new_admin_address"
          ],
          "properties": {
            "new_admin_address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_cooldown"
      ],
      "properties": {
        "update_cooldown": {
          "type": "object",
          "required": [
            "new_cooldown"
          ],
          "properties": {
            "new_cooldown": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_end_height"
      ],
      "properties": {
        "update_end_height": {
          "type": "object",
          "properties": {
            "new_end_height": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "Color": {
      "type": "string",
      "enum": [
        "white",
        "light_gray",
        "gray",
        "black",
        "pink",
        "red",
        "orange",
        "brown",
        "yellow",
        "light_green",
        "green",
        "turquoise",
        "light_blue",
        "blue",
        "magenta",
        "purple"
      ]
    }
  }
}
